{
  "extends": ["next/core-web-vitals", "plugin:@typescript-eslint/recommended", "next"],
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-no-undef": "off"
  }
}
